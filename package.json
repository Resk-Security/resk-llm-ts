{
  "name": "resk-llm-ts",
  "version": "0.6.0",
  "description": "A TypeScript library for secure LLM interactions.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "example:basic": "ts-node examples/basic_usage.ts",
    "example:express": "ts-node examples/express_integration.ts",
    "example:advanced": "ts-node examples/advanced_security_usage.ts",
    "example:vector": "ts-node examples/vector_db_setup.ts",
    "example:frontend-security": "ts-node examples/frontend_security_usage.ts",
    "example:multi-provider": "ts-node examples/multi_provider_usage.ts",
    "example:vector-persistence": "ts-node examples/vector_persistence_usage.ts",
    "test:frontend": "jest test/frontend_security.test.ts",
    "test:all": "jest --coverage",
    "build:prod": "tsc --project tsconfig.json && npm run lint",
    "prepublishOnly": "npm run build:prod && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Resk-Security/resk-llm-js"
  },
  "keywords": [
    "llm",
    "ai",
    "security",
    "openrouter",
    "resk",
    "prompt injection",
    "openai",
    "moderation"
  ],
  "author": "Resk contact@resk.fr",
  "license": "GPL-3.0 license",
  "bugs": {
    "url": "https://github.com/Resk-Security/resk-llm-js#issues"
  },
  "homepage": "https://github.com/Resk-Security/resk-llm-js#readme",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/jest": "^29.5.12",
    "@types/mathjs": "^9.4.2",
    "@types/node": "^20.12.12",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.31.1"
  },
  "dependencies": {
    "mathjs": "^12.4.2",
    "openai": "^4.47.1",
    "sanitize-html": "^2.17.0"
  },
  "files": [
    "dist/**/*"
  ]
}
